@model IEnumerable<Batch155SMSOnline3.Models.Customer>

@{
    ViewBag.Title = "Friends";
    Layout = "~/Areas/Admin/Views/_LayoutDashboard.cshtml";
}

<h2>Friends</h2>


<table class="table">
    <tr>
        <th>Username</th>
        <th>Customer Name</th>
        <th>Date Of Birth</th>
        <th>Email</th>
        <th>Phone Number</th>
        <th>Date Of Registration</th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.Username)</td>
            <td>@Html.DisplayFor(modelItem => item.CustomerName)</td>
            <td>@Html.DisplayFor(modelItem => item.DateOfBirth)</td>
            <td>@Html.DisplayFor(modelItem => item.Email)</td>
            <td>@Html.DisplayFor(modelItem => item.PhoneNumber)</td>
            <td>@Html.DisplayFor(modelItem => item.DateOfRegistration)</td>
            <td>@Html.ActionLink("Details", "Details", new { id = item.Id })</td>
            <td>@Html.ActionLink("Friends", "Friends", new { id = item.Id })</td>
        </tr>
    }
</table>
